                            <img src="http://img1.bitautoimg.com/autoalbum/files/20150709/800/13310080070739_4148544_3.jpg" />
                        </div>
                        <div class="swiper-slide">
                            <img src="http://img1.bitautoimg.com/autoalbum/files/20150709/792/13294679244661_4148524_3.jpg" />
                        </div>
            </div>
            <!-- Add Pagination -->
            <div class="swiper-pagination" style="display: none;"></div>
            <div class="fy"></div>
        </div>
    </div>

    <!-- 图片轮播结束 -->

    <blockquote class="list-info">
        <!-- 选车开始 -->
        <section class="selCar">
            <h1 id="selCar" class="car-name font-30 ut-s">宝骏560 2016款 1.8L 手动 舒适型</h1>
            <div class="uf uf-ic car-information font-26">
                <div class="uf-f1 car-company">北京4S店平均报价：</div>
                <div class="car-price ut-r ">￥73800)</div>
                            </div>
                        </section>
                        <!-- 选车结束 -->
                        <!-- 首付月供开始 -->
                        <section class="selBox">
                            <div class="sel-items">
                                <h2 class="font-26">首付</h2>
                                <div id="shoufu"></div>
                            </div>
                            <div class="sel-items">
                                <h2 class="font-26">期限</h2>
                                <div id="yuegong"></div>
                            </div>
                        </section>
                        <!-- 首付月供结束 -->
                        <!-- 详细信息开始 -->
                        <section class="info-box">
                            <div>
                                <div class="uf uf-ic font-26">
                                    <div class="uf-f2 font_blod">首付 <span class="font-24 col_grey6 font_no_blod">(车价 × <span data-bind="text:downPaymentPercentText">20</span>%)</span></div>
                                    <div class="uf-f1 ut-r col_rede">￥<span data-bind="text:downPayment">--</span></div>
                                </div>
                                <div class="uf uf-ic font-26">
