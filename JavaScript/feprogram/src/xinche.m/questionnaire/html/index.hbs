<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="UTF-8">
    <title>易鑫车贷调查问卷</title>
    <meta name="format-detection" content="telephone=no"/>
    <script src="{{htmlWebpackPlugin.options.common}}"></script>
</head>
<body>
{{{htmlWebpackPlugin.options.header}}}
<script>
    //设备
    var DeviceID=  "8f00a536-78ce-4681-ae67-82fe37872cf7";
    //终端
    var Termainal = "Termainnal" ;
    //城市
    var CityName = "Beijing" ;
